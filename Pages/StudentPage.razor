@page "/studentpage"

<h2>List Of Student</h2>
 <button class="btn btn-primary" onclick="window.location.href='/addStudent'">Tambah Mahasiswa</button>
 <a><br></a>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Nim</th>
            <th>Nama Belakang</th>
            <th>Nama Depan</th>
            <th>Tanggal Masuk</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var stud in Students)
        {
            <tr>
                <td>@stud.studentID</td>
                <td>@stud.lastName</td>
                <td>@stud.firstName</td>
                <td>@stud.enrollmentDate</td>
                <td>
                <a href="@($"detailpage/{stud.studentID}")" class="btn btn-outline-primary">details</a>
                <a href="/enrollment" class="btn btn-outline-warning">enroll</a>
                <a href="@($"editstudent/{stud.studentID}")" class="btn btn-outline-info">edit</a>
                <a href="@($"deletestudent/{stud.studentID}")" class="btn btn-outline-dark">delete</a></td>
            </tr>
        }
    </tbody>
</table>